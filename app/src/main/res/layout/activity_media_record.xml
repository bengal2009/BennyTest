<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.blin.bennytest.MEDIA.MediaRecordActivity">

    <TextView android:text="@string/hello_world" android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView5" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="StartRecord"
        android:onClick="StartRec"
        android:id="@+id/button91"
        android:layout_marginTop="146dp"
        android:layout_below="@+id/textView5"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="StopRecord"
        android:onClick="StopRec"
        android:id="@+id/button9"
        android:layout_alignTop="@+id/button91"
        android:layout_toRightOf="@+id/button91"
        android:layout_toEndOf="@+id/button91" />
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="PlayRecord"
        android:onClick="PlayRec"
        android:id="@+id/button92"
         />


</RelativeLayout>
